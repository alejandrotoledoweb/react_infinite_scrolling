{
  "name": "react-base-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1.2.1",
    "axios-mock-adapter": "^1.21.2",
    "classnames": "^2.3.2",
    "env-cmd": "^10.1.0",
    "mobx": "^6.7.0",
    "mobx-react": "^7.6.0",
    "react": "^18.0.25",
    "react-dom": "^18.0.9",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint-fix": "eslint \"src/**/*.{ts,tsx}\" --quiet --fix",
    "start": "react-scripts start",
    "start:local": "env-cmd -f .env.local react-scripts start",
    "start:development": "env-cmd -f .env.development react-scripts start",
    "start:staging": "env-cmd -f .env.staging react-scripts start",
    "start:production": "env-cmd -f .env.production react-scripts start",
    "build": "react-scripts build",
    "build:local": "env-cmd -f .env.local react-scripts build",
    "build:development": "env-cmd -f .env.development react-scripts build",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^29.2.2",
    "@types/node": "^16.11.20",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest-watch-typeahead": "^0.6.5",
    "prettier": "^2.5.1",
    "sass": "^1.54.3",
    "typescript": "^4.5.5"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!axios)/"
    ],
    "collectCoverageFrom": [
      "<rootDir>/server/**/*.{ts,tsx}",
      "<rootDir>/src/**/*.{ts,tsx}",
      "!src/**/*.{interfaces,styles,d}.{ts,tsx}",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/utils/interfaces/*"
    ]
  }
}
